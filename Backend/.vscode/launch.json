{
    "configurations": [
        {
            "type": "java",
            "name": "EditorbackendApplication",
            "request": "launch",
            "mainClass": "com.editorbackend.editorbackend.EditorbackendApplication",
            "projectName": "editorbackend"
        },
        {
            "name": "Debug C Program with Args",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/req4.c",
            "args": [
                "test.txt",
                "3",
                "40"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}